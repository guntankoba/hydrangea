  "compilerOptions": {
    "target": "ES2019",
    "module": "ES2020",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "docs/assets",
    "rootDir": "src"
  },
  "include": ["src"]
}
